circular_reference=->{
    tab=[
        message="You thought that this will be a destructor, but it is me DIO!"
        destroy=self->print(self.message)
    ]
    tab.self=tab
    tab
}
loop=(times ,f)->{
    f()
    if(times>0){
        loop(times-1, f)
    } 
}
loop(30, circular_reference)
collect_garbage()