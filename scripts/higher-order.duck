loop=(start, end ,f)->{
    f(start)
    if(start<end){
        loop(start+1, end, f)
    } 
}
adder=to_add->{
    n->n+to_add
}
printplusfive=n->{
    f=adder(5)
    print(f(n))
}
count=(arr)->{
    count_recursive=(arr, index)->{
        if(arr[index]!=null){
            1+count_recursive(arr, index+1)
        } else {
            0
        }
    }
    count_recursive(arr, 0)
}
count([1, 2, 3])

loop(1, 3, printplusfive)