@require=predicate->{
    @quote_with
    if(!@predicate){
        new_error('REQUIRE_ERROR, {@quote, @predicate}, "Require predicate failed")!
    }
    ['predicate]
}
@require_type=(variable, type)->{
    @quote_with
    if(!match(@variable, @type)){
        new_error('REQUIRE_TYPE_ERROR, [@variable, @type], "Require type failed")!
    }
    ['variable, 'type]
}
f=x->{
    @require_type, x, types.int
    @require, x>0
    x*x
}
f(-10)